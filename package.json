{
  "name": "goodchords",
  "repository": "git@github.com:mrfrac/goodchords.git",
  "author": "mrfrac <fracturer@gmail.com>",
  "version": "0.5.0",
  "description": "Music theory library",
  "main": "dist/goodchords.cjs.js",
  "module": "dist/goodchords.es.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": "^20.9"
  },
  "scripts": {
    "build": "nx rollup:build",
    "test": "nx jest:test",
    "testing": "nx jest:test --watch",
    "coverage": "nx jest:test --coverage",
    "clean": "rimraf dist coverage",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "linter": "npx eslint --ignore-path .gitignore . --fix",
    "lint": "nx run-many --target=lint --all"
  },
  "keywords": [
    "chord",
    "note",
    "interval",
    "scale",
    "music"
  ],
  "devDependencies": {
    "@nx/eslint": "20.6.2",
    "@nx/eslint-plugin": "20.6.2",
    "@nx/jest": "20.6.2",
    "@nx/js": "20.6.2",
    "@nx/rollup": "20.6.2",
    "@rollup/plugin-terser": "0.4.4",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/jest": "29.5.14",
    "@types/node": "20.14.8",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "coveralls": "3.1.1",
    "eslint": "~8.57.0",
    "eslint-config-prettier": "9.1.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-environment-node": "29.7.0",
    "jsonc-eslint-parser": "2.4.0",
    "lint-staged": "14.0.1",
    "nx": "20.6.2",
    "prettier": "3.3.3",
    "rollup": "4.36.0",
    "rollup-plugin-typescript2": "0.36.0",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.1",
    "tslib": "2.6.2",
    "typescript": "5.5.4"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --parser typescript --write --trailing-comma all --arrow-parens always",
      "npm run typecheck",
      "npm run linter"
    ]
  },
  "nx": {},
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "nx"
    ],
    "ignoredBuiltDependencies": [
      "@swc/core"
    ]
  },
  "dependencies": {}
}
